Host ela
    HostName ela.cscs.ch
    User sstapf 
    ForwardAgent yes
    ForwardX11 yes
    forwardX11Trusted yes
	IdentityFile ~/.ssh/cscs-key


Host todi
    HostName todi.cscs.ch
    User sstapf
    ProxyJump ela
    ForwardAgent yes
    ForwardX11 yes
    forwardX11Trusted yes
	IdentityFile ~/.ssh/cscs-key

